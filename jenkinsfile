pipeline { 
    agent any  
    stages { 
        stage('Build') { 
            steps { 
	       echo '1st stage'
               echo 'This is a pipeline code' 
            }
        }
	stage('Linux_Comand') {
	    steps {
		echo '2nd stage'
                sh 'mkdir /tmp/pipeline1'
	    }
	}
    }
}
